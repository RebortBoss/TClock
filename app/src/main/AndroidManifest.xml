<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jkt.clock">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".itemactivities.ShowAllActivity">
        </activity>
        <activity android:name=".itemactivities.NoSmallCircleActivity">
        </activity>
        <activity android:name=".itemactivities.NoArcActivity">
        </activity>
        <activity android:name=".itemactivities.NoShaderActivity">
        </activity>
        <activity android:name=".itemactivities.NoMsgActivity">
        </activity>
        <activity android:name=".itemactivities.NoNumActivity">
        </activity>
        <activity android:name=".itemactivities.NoPointActivity">
        </activity>
        <activity android:name=".itemactivities.CustomColorActivity">
        </activity>
        <activity android:name=".itemactivities.CustomSizeActivity">
        </activity>
        <activity android:name=".itemactivities.BaseDataActivity">
        </activity>
        <activity android:name=".itemactivities.CustomMsgActivity">
        </activity>
    </application>

</manifest>
